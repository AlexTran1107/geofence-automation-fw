<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Geofence Mobile Automation Suite" parallel="false" verbose="2">
    <listeners>
        <listener class-name="com.automation.listeners.TestListener"/>
        <listener class-name="com.automation.listeners.AllureListener"/>
    </listeners>

    <test name="Geofence E2E Tests">
        <classes>
            <class name="com.automation.tests.GeofenceLocationSetupTest"/>
            <class name="com.automation.tests.ExitGeofenceTest"/>
        </classes>
    </test>
</suite>



